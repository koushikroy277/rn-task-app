{
  "expo": {
    "name": "NoteItAll",
    "slug": "NoteItAll",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4ea4de"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.noteitall.noteitall",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4ea4de"
      },
      "useNextNotificationsApi": true,
      "package": "com.noteitall.noteitall",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "React Native Task Writer and Task management App using Expo Cli\n",
    "githubUrl": "https://github.com/koushikroy277/rn-task-app.git"
  }
}
